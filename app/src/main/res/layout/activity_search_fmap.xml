<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#f5f5f5"
    android:clipToPadding="true"
    android:fitsSystemWindows="true"
    android:orientation="vertical">

    <com.zgy.parking.indoor.view.UITitleBackView
        android:id="@+id/search_map_uib"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        app:uibRightView="textvisiable">
    </com.zgy.parking.indoor.view.UITitleBackView>


    <TextView
        android:id="@+id/tv_parking_search_car"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="#33000000"
        android:gravity="center"
        android:text="停车场"
        android:textColor="@color/color_white"
        android:textSize="14sp" />



    <RelativeLayout
        android:layout_width="match_parent"
        android:background="@color/color_white"
        android:layout_height="50dp">
        <com.zgy.parking.indoor.utils.SearchBar
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:id="@+id/search_title_bar_map"
            android:layout_width="match_parent"
            android:layout_height="50dp"></com.zgy.parking.indoor.utils.SearchBar>
    </RelativeLayout>



    <com.fengmap.android.map.FMMapView
        android:id="@+id/mapview_search_fmap"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></com.fengmap.android.map.FMMapView>



</LinearLayout>